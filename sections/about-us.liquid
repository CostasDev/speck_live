<div class="sp-bs-sol-top-banner sp-about-us-pg-banner sp-bs-sol-top-banner-black-lyr">  
	{%if section.settings.banner != blank %}
  <div class="sp-bs-sol-top-banner-inn" style="background-image: url('{{section.settings.banner | img_url: 'master'}}')"></div>
  {%endif%}
  	<div class="sp-bs-sol-top-banner-cont">
        {%if section.settings.banner-text != blank %}
		<h3>{{section.settings.banner-text}}</h3>
        {%endif%}
	</div>  
</div>


{%if section.settings.banner != blank %}
<div class="sp-abt-us-main-desc">
	<div class="sp-abt-us-inn-desc">
		<p>{{section.settings.banner-bellow-text}}</p>
	</div>
</div>
{%endif%}
<div class="sp-abt-us-flw-sec">
	<div class="sp-abt-us-flw-row">
		<div class="sp-abt-us-flw-block">
			<div class="sp-abt-us-flw-img">
              {%if section.settings.our_promise_image_one != blank %}
				<img data-src="{{section.settings.our_promise_image_one | img_url: 'master'}}" class="sp-desktop-show-dt lazyload" />
              {%endif%}
              
                {%if section.settings.our_promise_image_two != blank %}
				<img data-src="{{section.settings.our_promise_image_one_mob | img_url: 'master'}}" class="sp-mob-show-dt lazyload" />
              {%endif%}
			</div>
		</div>
         {%if section.settings.our_promise_content != blank %}
		<div class="sp-abt-us-flw-block">
			<div class="sp-abt-us-flw-cont">
				{{section.settings.our_promise_content}}
			</div>
		</div>
      {%endif%}
		<div class="sp-abt-us-flw-block">
			<div class="sp-abt-us-flw-img"> 
        <img src="{{section.settings.our_promise_image_two | img_url: 'master'}}" class="sp-desktop-show-dt" />
				<img src="{{section.settings.our_promise_image_two_mob | img_url: 'master'}}" class="sp-mob-show-dt" />
				{% comment %} <img src="https://cdn.shopify.com/s/files/1/0325/4252/3523/files/abt-op-img-02.jpg?v=1641895556" class="sp-desktop-show-dt" />
				<img src="https://cdn.shopify.com/s/files/1/0325/4252/3523/files/abt-op-img-02-mob.jpg?v=1641895556" class="sp-mob-show-dt" /> {% endcomment %}
			</div>
		</div>
	</div>
</div>
<div class="sp-abt-us-or-team-sec">
	<div class="sp-abt-us-or-team-inn">
     
		<div class="sp-abt-us-or-team-cont">
           {%if section.settings.our-team-heading != blank %}
			<h4>{{section.settings.our-team-heading}}</h4>
          {%endif%}
          {%if section.settings.our-team-content != blank %}          
			<p>{{section.settings.our-team-content}}</p>
          {%endif%}
		</div>
      
      {%if section.settings.video-url != blank %}
		<div class="sp-abt-us-or-team-video">  
           <video width="" height="" autoplay controls>
              <source src="{{section.settings.video-url }}" type="video/mp4">
              Your browser does not support the video tag.
            </video> 
			<!--video here-->
		</div>		
      {%endif%}
	</div>
</div>
