<div class="sp-gb-page-title-main">
	<div class="sp-container-1140">
		<h1>{{section.settings.head_1}}</h1>
		<div class="sp-ppcc-upr-video-main">
			<div class="sp-ppcc-upr-video-row">
				<div class="sp-ppcc-upr-video-block">
                  <div class="sp-ppcc-upr-video-con">
                  <iframe width="560" height="315" src="{{section.settings.vid_1}}" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen="" id="widget2"></iframe>
                  </div>
                  </div>
				<div class="sp-ppcc-upr-cont-block">
					<h4>{{section.settings.head_2}}</h4>
					<p>{{section.settings.head_3}}</p>
				</div>
			</div>
			<div class="sp-ppcc-upr-video-btn"><a href="{{section.settings.btn_1}}" class="sp-bl-back-btn">{{section.settings.btn_link_1}}</a></div>
			<div class="sp-ppcc-upr-video-desc"><p>{{section.settings.btn_desc_1}}</p></div>
		</div>
	</div>
	<div class="sp-ppcc-fea-sec-main">
		<div class="sp-container-1140">
			<div class="sp-ppcc-fea-sec-row">
				<div class="sp-ppcc-fea-sec-img"><img class="lazyload" data-src="{{section.settings.img_1 | img_url:'master'}}" /></div>
				<div class="sp-ppcc-fea-sec-cont">
					<div class="sp-ppcc-fea-sec-cont-row">
						<div class="sp-ppcc-fea-sec-cont-img"><img class="lazyload" data-src="{{section.settings.icon_img_1 | img_url:'master'}}" /></div>
						<div class="sp-ppcc-fea-sec-cont-det">
							<h4>{{section.settings.icon_head_1}}</h4>
							<p>{{section.settings.icon_text_1}}</p>
						</div>
					</div>
					<div class="sp-ppcc-fea-sec-cont-row">
						<div class="sp-ppcc-fea-sec-cont-img"><img class="lazyload" data-src="{{section.settings.icon_img_2 | img_url:'master'}}" /></div>
						<div class="sp-ppcc-fea-sec-cont-det">
							<h4>{{section.settings.icon_head_2}}</h4>
							<p>{{section.settings.icon_text_2}}</p>
						</div>
					</div>
					<div class="sp-ppcc-fea-sec-cont-row">
						<div class="sp-ppcc-fea-sec-cont-img"><img class="lazyload" data-src="{{section.settings.icon_img_3 | img_url:'master'}}" /></div>
						<div class="sp-ppcc-fea-sec-cont-det">
							<h4>{{section.settings.icon_head_3}}</h4>
							<p>{{section.settings.icon_text_3}}</p>
						</div>
					</div>
				</div>
			</div>
			<div class="sp-meet-fam-sec">
				<h2>{{section.settings.head_1_1}}</h2>
				<div class="sp-meet-fam-row">
					<div class="sp-meet-fam-img">
                      <img class="lazyload" data-src="{{section.settings.img_1_new | img_url:'master'}}" />
                      <h3>{{section.settings.img_head_1_new}}</h3>
                    </div>
					<div class="sp-meet-fam-cont">
						<h3>{{section.settings.img_head_1_new}}</h3>
						<p>{{section.settings.txtttt_1}}</p>
						<div class="sp-meet-fam-cont-btn"><a href="{{section.settings.btn_link_1_new}}" class="sp-bl-back-btn">{{section.settings.btn_txt_1_new}}</a></div>
					</div>
				</div>
				<div class="sp-meet-fam-row">
					<div class="sp-meet-fam-img">
                      <img class="lazyload" data-src="{{section.settings.img_1_2_new | img_url:'master'}}" />
                  	  <h3>{{section.settings.img_head_2_new}}</h3>	
                    </div>
					<div class="sp-meet-fam-cont">
						<h3>{{section.settings.img_head_2_new}}</h3>
						<p>{{section.settings.txtttt_2}}</p>
						<div class="sp-meet-fam-cont-btn"><a href="{{section.settings.btn_link_2_new}}" class="sp-bl-back-btn">{{section.settings.btn_txt_2_new}}</a></div>
					</div>
				</div>
				<div class="sp-meet-fam-row">
					<div class="sp-meet-fam-img">
                      <img class="lazyload" data-src="{{section.settings.img_1_3_new | img_url:'master'}}" />
                      <h3>{{section.settings.img_head_3_new}}</h3>
                    </div>
					<div class="sp-meet-fam-cont">
						<h3>{{section.settings.img_head_3_new}}</h3>
						<p>{{section.settings.txtttt_3}}</p>
