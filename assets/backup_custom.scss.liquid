                            flex: 0 0 auto;
                            margin-bottom: auto;
                            display: flex;
                            flex-wrap: wrap;
                            @media screen and (max-width: 768px) {
                                padding: 0;
                                margin: 0 -7px;
                                width: calc(100% + 14px);
                            }
                            .tagalys-product-tile{
                                text-align: left;
                                .product-link{
                                    border: none;
                                    .product-name{
                                        overflow: initial;
                                        white-space: initial;
                                        margin-bottom: 10px;
                                        height: auto;
                                        font-weight: bold;
                                    }
                                    .pr-snippet{
                                        margin-top: 10px;
                                        .pr-category-snippet__rating{
                                            .pr-snippet-stars{
                                                .pr-rating-stars{
                                                    display: flex;
                                                    .pr-star-v4{
                                                        border-radius: 3px;
                                                        height: 20px;
                                                        margin-left: 0;
                                                        margin-right: 2px;
                                                        padding: 3px;
                                                        width: 20px;
                                                        &.pr-star-v4-100-filled{
                                                            background-image: url(data:image/svg+xml;base64,);
                                                        }
                                                        &.pr-star-v4-25-filled {
                                                            background-image: url(data:image/svg+xml;base64,);
                                                        }
                                                        &.pr-star-v4-75-filled {
                                                            background-image: url(data:image/svg+xml;base64,);
                                                        }
                                                        &.pr-star-v4-50-filled {
                                                            background-image: url(data:image/svg+xml;base64,);
                                                        }
                                                    }
                                                }
                                                .pr-snippet-rating-decimal{
                                                    display: none;
                                                }
                                            }
                                            .pr-accessible-text{
                                                display: none;
                                            }
                                        }
                                        .pr-category-snippet__total{
                                            display: none;
                                        }
                                    }
                                }
                            }
                        }
                    }
                    .tile-container{
                        .grid-product__tag {
                            font-size: calc(var(--typeBaseSize)*.65);
                            position: absolute;
                            top: 0;
                            right: 0;
                            line-height: 1;
                            padding: 6px 5px 6px 7px;
                            background-color: #111;
                            background-color: var(--colorBtnPrimary);
                            color: #fff;
                            color: var(--colorBtnPrimaryText);
                            z-index: 2;
                            transition: opacity .4s ease;
                            &.grid-product__tag--custom{
                                background: rgba(255, 255, 255, 0.5);
                                backdrop-filter: blur(4px);
                                border-radius: 4px;
                            }
                        }
                    }
                }
            }
            .product-actions-quick-links-icon {
                &:hover {
                    cursor: pointer;
                }
            }
            .product-actions-link {
                .dropdown-menu {
                    display: none;
                    position: absolute;
                    top: 24px;
                    background: #fff;
                    border: 1px solid #ccc;
                    width: 100px;
                    border-radius: 3px;
                    z-index: 1000;
                }
                li {
                    display: block;
                    border-bottom: 1px solid #ccc;
                    &:last-child {
                        border-bottom: 0;
                    }
                    a {
                        display: block;
                        padding: 0.5em;
                        text-align: left;
                        &:hover {
                            background: #f6f6f6;
                        }
                        &:active {
                            background: #666;
                            color: #fff;
                        }
                    }
                }
                &:hover {
                    .dropdown-menu {
                        display: block;
                    }
                }
            }
            .dropdown-menu.tscore-info {
                li {
                    display: block;
                    border-bottom: 1px solid #ccc;
                    display: block;
                    padding: 0.5em;
                    text-align: left;
                }
            }
        }
    }
}



