@font-face {
    font-family: 'Texta';
    src: url('Texta-Regular.eot');
    src: local('Texta'), local('Texta-Regular'),
        url('Texta-Regular.eot?#iefix') format('embedded-opentype'),
        url('Texta-Regular.woff2') format('woff2'),
        url('Texta-Regular.woff') format('woff'),
        url('Texta-Regular.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'Texta';
    src: url('Texta-Bold.eot');
    src: local('Texta Bold'), local('Texta-Bold'),
        url('Texta-Bold.eot?#iefix') format('embedded-opentype'),
        url('Texta-Bold.woff2') format('woff2'),
        url('Texta-Bold.woff') format('woff'),
        url('Texta-Bold.ttf') format('truetype');
    font-weight: bold;
    font-style: normal;
}
@font-face {
    font-family: 'Texta';
    src: url('Texta-Medium.eot');
    src: local('Texta Medium'), local('Texta-Medium'),
        url('Texta-Medium.eot?#iefix') format('embedded-opentype'),
        url('Texta-Medium.woff2') format('woff2'),
        url('Texta-Medium.woff') format('woff'),
        url('Texta-Medium.ttf') format('truetype');
    font-weight: 500;
    font-style: normal;
}
@font-face {
    font-family: 'Texta';
    src: url('Texta-Heavy.eot');
    src: local('Texta Heavy'), local('Texta-Heavy'),
        url('Texta-Heavy.eot?#iefix') format('embedded-opentype'),
        url('Texta-Heavy.woff2') format('woff2'),
        url('Texta-Heavy.woff') format('woff'),
        url('Texta-Heavy.ttf') format('truetype');
    font-weight: 900;
    font-style: normal;
}


a {
    /* outline: none !important; */
}

*:focus { outline: 2px solid #000 !important; }
*:focus:not(:focus-visible), *:focus-visible { outline: none; }
    
.shopify-section--icon-list{
    .section-header{
        margin: 50px 0;
        @media screen and (max-width: 768px) {
            margin: 20px 0;
        }
    }
    .logo-bar{
        @media screen and (max-width: 768px) {
            margin-bottom: -10px;
        }
        .logo-bar__item{
            
            &.item-align--left{
                display: flex;
                align-items: center;
                flex: 0 1 200px;
                @media screen and (max-width: 768px) {
                    flex: 0 1 25%;
                    flex-wrap: wrap;
                    justify-content: center;
                }
                img.logo-bar__image{
                    width: 40px;
                    margin-right: 10px;
                    image-rendering: -webkit-optimize-contrast;
                    @media screen and (max-width: 768px) {
                        margin: auto;
                    }
                }
            }
            &.item-align--bottom {
                flex: 0 1 80px;
                list-style: none;
                @media screen and (max-width: 768px) {
                    flex: 0 1 calc(25% - 35px);
                }
            }
            span{
                display: block;
                @media screen and (max-width: 768px) {
                    line-height: 14px;
                    margin-top: 10px;
                }
                em {
                    display:block;
                    margin:0;
                    padding:0;
                    font-size:80%;
                    line-height:0.7
                }
            }
        }
    }
}

.section--divider-bottom{
    border-bottom: 1px solid var(--colorBorder);
}

.section--divider-top{
    border-top: 1px solid var(--colorBorder);
}
.icon-list {
    padding: 10px 0;
}

.collection-item{
    margin-bottom: 20px;
    @media screen and (max-width: 768px) {
        .collection-image--square{
            padding-bottom: 68%;
        }
    }
    .collection-item__title{
        max-width: 300px;
        width: 100%;
    }
}
strong{
    font-weight: 900;
}
.normal-weight{
    font-weight: normal;
}
.small-size{
    font-size: 0.7em;
}
.left-chevron{
    display: flex;
    align-items: center;
    width: fit-content;
    position: relative;
    padding-right: 10px;
    &::after{
        content: " ";
        width: 4px;
        height: 4px;
        border: 1px solid;
        border-bottom: none;
        border-left: none;
        display: block;
        transform: rotate(45deg);
        order: 1;
        margin-left: 10px;
    }
    &::before{
        content: " ";
        width: 4px;
        height: 4px;
        border: 1px solid;
        border-bottom: none;
        border-left: none;
        display: block;
        transform: rotate(45deg);
        position: absolute;
        right: 5px;
    }
}

.link{
    color: var(--colorLink);
    font-size: 14px;
    font-weight: 900;
    letter-spacing: 0.93px;
    text-transform: uppercase;
}

.section-header{
    position: relative;
    .section-header__link{
        position: absolute;
        right: 0;
        top: 0;
        margin-top: 0;
        line-height: 23.8px;
        text-transform: uppercase;
        color: var(--colorLink);
        font-size: 14px;
        font-weight: 900;
        letter-spacing: 0;
    }
    .section-header__title{
        &.large{
            font-size: 44px;
            font-weight: normal;
            @media screen and (max-width: 768px) {
                font-size: 34px;
            }
        }
    }
}

.logolist-section{
    .logo-slider{
        max-width: 700px;
        margin: auto;
        .logo-slide{
            text-align: center;
            width: 100%;
            color: #6A6C68;
            font-size: 24px;
            line-height: 31px;
            font-weight: bold;
            @media screen and (max-width: 768px) {
                font-size: 18px;
                line-height: 24px;
            }
        }
    }

    .logo-bar{
        margin-top: 20px;
        .logo-bar__item{
            flex: 0 1 140px;
            margin: 0 10px 30px;
            @media screen and (max-width: 768px) {
                flex: 0 1 calc(25% - 20px);
                svg{
                    width: 100%;
                }
            }
            svg{
                opacity: 0.4;
            }
            a{
                &.is-active{&:before{
                    content: none;
                }
                    svg{
                        opacity: 1;
                    }
                }
            }
        }
    }
}

.collection-grid__wrapper{
    .collection-filter{
        margin-bottom: 20px;
    }
    .collection-description-bottom {
      padding: 0 0 6em;
    }
}

.grid {
  li {
    list-style: none;
  }
}

.grid-product{
    .grid-product__colors {
        justify-content: flex-start;
        @media screen and (max-width: 768px) {
            padding-left: 11px;
        }
        .color-swatch{
            margin-left: 0;
            &::before{
                border: none!important;
            }
        }
    }
    .grid-product__title {
        h2 {
            text-transform: none;
            font-size: calc(var(--typeBaseSize) + 4px);
            letter-spacing: normal;
            font-weight: 700;
            word-break: break-word;
            -webkit-hyphens: auto;
            hyphens: auto;
        }
    }
}

.grid-product__title-container {
  padding-bottom: 0;
}

.grid-product__info-container {
  padding-top: 0;
}

.collection-hero__content{
    align-items: flex-end!important;
    .section-header--hero{
        .section-header__shadow{
            .collection__description{
                margin-top: 20px;
                font-size: 18px;
                max-width: 500px;
                width: 100%;
                font-weight: normal;
            }
        }
    }
}

.product-single__meta{
    max-width: 545px;
    margin: auto;
    @media screen and (max-width: 768px) {
        margin-top: 20px;
    }
    .product-single__title{
        font-size: 36px;
        font-weight: 400;
        @media screen and (max-width: 768px) {
            font-size: 24px;
        }
    }
    .product-single__vendor{
        a{
            background: #e8e8e1;
            border-radius: 5px;
            padding: 0 8px;
            letter-spacing: normal;
            text-transform: none;
            line-height: 19px;
        }
    }
    .product-single__sku{
        font-weight: normal;
        font-size: 13px;
    }
    .product-block{
        .breadcrumb{
            opacity: 0.5;
        }
        iframe{
            @media screen and (max-width: 768px) {
                    margin: auto;
                    display: block;
            }
        }
        #pwr-snippets {
            margin-top: 30px;
            .pr-snippet{
                .pr-snippet-stars-container{
                    .pr-snippet-stars{
                        .pr-rating-stars{
                            .pr-star-v4{
                                
                            }
                        }
                        .pr-snippet-rating-decimal{
                            display: none;
                        }
                    }
                }
                .pr-snippet-read-and-write{
                    margin-top: 0;
                    a{
                        font-size: 0!important;
                        color: transparent;
                        &.pr-snippet-write-review-link{
                            display: none;
                        }
                        &::before{
                            content: "-";
                            font-size: 12px;
                            color: black;
                            margin-right: 5px;
                        }
                        &::after{
                            content: "Read";
                            font-size: 12px;
                            color: black;
                            margin-right: 5px;
                        }
                    }
                }
            }
        }
        &.section--divider-top{
            padding-top: 20px;
        }
        .variant-wrapper{
            .variant__label{
                text-transform: capitalize;
                letter-spacing: normal;
            }
            .variant-input-wrap{
                .variant-input{
                    input[type=radio] {
                        
                        &:checked+label {
                            box-shadow: 0 0 0 2px #007E9E;
                            color: #007E9E;
                            &.color-swatch{
                                box-shadow: 0 0 0 2px #9da1a2;
                            }
                        }
                    }
                    label {
                        box-shadow: 0 0 0 2px transparent;
                        background-color: #F5F6F5;
                        &.color-swatch{
                            height: 30px;
                            width: 30px;
                            overflow: initial;
                        }
                    }
                    
                }
                & + .variant__label {
                    margin-top: 5px;
                }
            }
        }
        &.product-block--price{
            span{
                font-size: calc(var(--typeHeaderSize) - 4px);
                font-weight: bold;
            }
        }
        .pucharge-bar{
            display: flex;
            #ZlistWishlist{
                margin: 0;
                width: auto;
                margin-left: 10px;
                label{
                    margin: 0!important;
                    .zooomypwishmsg{ 
                        height: 40px;
                        display: flex;
                        align-items: center;
                        justify-content: center;
                        i{
                            padding: 0!important;
                        }
                    }
                }
                .emptymsg{
                    display: none;
                }
            }
            .product-block{
                margin: 0;
                margin-right: 5px;
                .js-qty__wrapper{
                    min-width: 120px;
                    .js-qty__num{
                        min-height: 40px;
                    }
                }
            }
            .add-to-cart{
                background: var(--colorCTABtn);
                height: 40px;
                padding: 0;
                span{
                    letter-spacing: normal;
                    font-size: 15px;
                    font-weight: 900;
                }
                &.sold-out {
                    background: var(--colorModalBg);
                    color: var(--colorTextBody);
                    &:hover {                          
                        background: rgba(204,204,204,0.3) !important;
                    }
                }
                &.coming-soon {
                    background: var(--colorModalBg);
                    color: #0ba200;
                    &:hover {                          
                        background: rgba(204,204,204,0.3) !important;
                    }
                }
            }
        }
        .shopify-section--icon-list{
            .icon-list{
                padding: 0;
                &.horizontal--only{
                    @media screen and (max-width: 768px) {
                        .logo-bar{
                            justify-content: flex-start;
                            .logo-bar__item{
                                flex: 0 1 50%;
                                flex-wrap: nowrap;
                                margin-bottom: 10px;
                                justify-content: flex-start;
                                img{
                                    margin-left: 0;
                                    margin-right: 10px;
                                }
                                span{
                                    max-width: 100px;
                                    text-align: left;
                                    margin: 0;
                                }
                            }
                            
                        }
                    }
                    
                }
            }
            .logo-bar{
                margin-bottom: 20px;
                .logo-bar__item{
                    display: flex;
                    align-items: center;
                    flex: 0 1 33.3%;
                    margin: 0;
                    .logo-bar__image{
                        min-width: 32px;
                        flex: 0 1 32px;
                        margin-right: 10px;
                        image-rendering: -webkit-optimize-contrast;
                        @media screen and (max-width: 768px) {
                            margin-right: auto;
                        }
                    }
                    span{
                        display: block;
                        width: 100%;
                        text-align: left;
                        font-size: 13px;
                        line-height: 13px;
                        @media screen and (max-width: 768px) {
                            text-align: center;
                        }
                    }
                }
            }
        }
        .product-message {
          margin: 1.5em 0 3em;
          font-size: 1.15em;
          p {
            line-height: 1;
          	a {
            	color: var(--colorCTABtn);
                &:hover {
                  text-decoration: underline;
                }
          	}
          }
        }
    }
}

.full-width-detail--section{
    padding: 50px 0;
    @media screen and (max-width: 768px) {
        background: #f5f6f5;
        padding: 50px 20px;
    }
    .tab-list{
        display: flex;
        justify-content: center;
        margin: 0;
        margin-bottom: 30px;
        @media screen and (max-width: 768px) {
            font-size: 20px;
            margin: 0;
        }
        button{
	        font-size: 24px;
    	    font-weight: 900;
        	text-transform: uppercase;
            text-align: center;
            border-bottom: 2px solid transparent;
            width: fit-content;
            margin: 0 15px;
            padding-bottom: 0px;
            cursor: pointer;
            &.active{
                border-color: #007E9E;
                color: #007E9E;
            }
        }
    }
    .tab-panels{
        .product-block{
            display: none;
            &.active{
                display: block;
            }
            .pc-primary {
                display:inline-block;
                .pcp-item {
                    display:block;
                    padding-top:3em;
                    .image, .content {
                        display:inline-block;
                        vertical-align:middle;
                        background-clip:padding-box;
                        box-sizing:border-box;
                    }
                    .image {
                        width:34.5%;
                        padding:0 4% 0 14%;
                        
                        img {
                            width:100%;
                        }
                    }
                    .content {
                        width:64.5%;
                        padding:0 12% 0 0;

                        p {
                            font-size:1em;
                            &.heading {
                                font-size:130%;
                                /*BS ADDed*/
                                font-size: 20px;
                                font-weight: bold;
                                sup {
                                    top:5px;
                                }
                            }
                        }
                        sup {
                            top:4px;
                            left:-1px;
                        }
                        small {
                            font-size: 75%;
                        }
                    }
                    &.speck-travel{
                        .image {
                            width:43%;
                            img {
                                border-radius:6px;
                                overflow:hidden;
                            }
                        }
                        .content {
                            width:56.5%;
                        }
                    }
                }
            }
            
            .pc-secondary{
                background: no-repeat top left / contain url('https://www.speckproducts.com/on/demandware.static/-/Sites-speck-Library/default/dw7d77f14c/premium-content/2020/pc_secondary-divide_01.png');
                padding:4em 5% 0;
                margin-top:3em;
                .pcs-item {
                    float:left;
                    width:33.33333%;
                    padding:0 4.5%;
                    box-sizing:border-box;
                    .image, .content {
                        box-sizing:border-box;
                    }
                    .image {
                        padding:0 25%;
                    }
                    .content {
                        text-align:center;
                        padding-top:0.25em;
                        p {
                            font-size:0.9em;
                            &.link {margin-top:0.3em;}
                            a, a:visited {
                                color:#09c;
                                text-decoration:none;
                            }
                            a:hover {
                                text-decoration:underline;
                            }
                        }
                        small {
                            font-size: 60%;
                            sup {
                                top: 2px;
                                margin-right: 2px;
                            }
                        }
                        /*BS Added*/
                        .heading{
                            font-size: 20px;
                            font-weight: 600;
                        }   
                    }
                }
            }
            
            .wave {
                background: no-repeat bottom left / contain url('https://www.speckproducts.com/on/demandware.static/-/Sites-speck-Library/default/dwf2505fe8/premium-content/2020/btm_swoosh_01.png');
                padding-bottom:14em;
            }
            
            
            @media only all and (max-width: 1280px) and (min-width: 768px) {
                .pc-primary .pcp-item .content p {font-size:0.9em;}
                .pc-primary .pcp-item .content p.heading {font-size:125%;}
                .pc-secondary .pcs-item {padding:0 3%;}
            }
            /* TABLET LANDSCAPE */
            @media only all and (max-width: 1280px) and (min-width: 1024px) {
                /* Primary Feature Section Item */
                .pc-primary .pcp-item .image {padding:0 5% 0 14%;}
                .pc-primary .pcp-item .content {padding:0 12% 0 0;}
                .pc-secondary .pcs-item .image {padding:0 28%;}
                .wave {padding-bottom:14em;}
            }
            /* TABLET PORTRAIT  */
            @media only all and (max-width: 1023px) and (min-width: 768px) {
                .pc-primary .pcp-item .image {padding:0 5% 0 8%;}
                .pc-primary .pcp-item .content {padding:0 6% 0 0;}
                .pc-primary .pcp-item.speck-travel .image {width:100%;padding:0 10%;}
                .pc-primary .pcp-item.speck-travel .content {width:100%;padding:1em 10% 0;}
                .pc-secondary .pcs-item .content p {font-size:1.3vw;}
                .wave {padding-bottom:12em;}
            }
            /* PHONE */
            @media only all and (max-width: 767px) {
                .pc-primary .pcp-item {margin-top:0;}
                .pc-primary .pcp-item .image, .pc-primary .pcp-item .content {width:100%;}
                .pc-primary .pcp-item .image {padding:0 30%;}
                .pc-primary .pcp-item .content {padding:1em 10% 0;text-align:center;}
                .pc-primary .pcp-item.speck-travel .image {width:100%;padding:0 10%;}
                .pc-primary .pcp-item.speck-travel .content {width:100%;padding:1em 10% 0;}
                .pc-secondary {padding-top:2em;}
                .pc-secondary .pcs-item {width:100%;margin-top:3em;padding:0;}
                .pc-secondary .pcs-item .image {padding:0 34%;margin-bottom:0.5em;}
                .pc-secondary .pcs-item .content p {font-size:1.1em;}
                .pc-secondary .pcs-item .content p.heading {font-size:130%;}
                .wave {padding-bottom:9em;}
            }
            /* PHONE (SMALL) */
            @media only all and (max-width: 480px) {
                .pc-primary .pcp-item .content p {font-size:4vw;}
                .pc-primary .pcp-item .content p.heading {font-size:4.7vw;font-weight: bold;}
                .pc-secondary .pcs-item {margin-top:2em;}
                .pc-secondary .pcs-item .content p {font-size:3.7vw;}
                .pc-secondary .pcs-item .content p.heading {font-size:4vw;}
                .wave {padding-bottom:6em;}
            }

            .feature-content-tab {
                background: #fff no-repeat bottom left / contain url('https://cdn.shopify.com/s/files/1/0325/4252/3523/files/btm_swoosh_01.jpg?v=1651190558');
                padding:3em 5% 10em;
                @media screen and (max-width: 768px) {}
            }
            .feature-content-tab p, .overview-tab p {color:#666;}   
            .feature-content-tab a, .overview-tab a, .feature-content-tab a:visited, .overview-tab a:visited {color:#09c;}
            .feature-content-tab .speck, .overview-tab .speck {color:#09c;}
            .feature-content-tab sup, .overview-tab sup {position:relative;top:4px;}
            /* Feature Tab */
            .feature-content-tab h3 {font-size:1.4em;margin-bottom:0.5em;}
            .feature-content-tab ul {padding-left:18px;}
            .feature-content-tab ul.specs {margin:2em 0;padding-left:0;}
            .feature-content-tab ul.specs li {list-style-type:none;}
            .feature-content-tab li {color:black;line-height:1.4;font-size:1.25em;list-style-type:disc;margin-top:0.5em;}
            .feature-content-tab li::marker{
                color: #09c;
            }
            .feature-content-tab li:first-child {margin-top:0;}
            .feature-content-tab li > span {color:#666;}
            .feature-content-tab .headline{font-size: 25px; margin-bottom: 12px;}
            /* Overview Tab */
            .feature-content-tab p {display:inline-block;font-size:1.25em;line-height:1.4;margin-top:1em;margin-bottom:0;}
            .feature-content-tab p:first-child {margin-top:0;}
            /* Award Section */
            .overview-tab .awardSection p {text-align:center;font-style: italic;font-size: 80%;padding: 0 10%;box-sizing:border-box;}
            .overview-tab .awardSection {display: flex;justify-content: space-between;width: 80%;margin: 50px auto 0;}
            .overview-tab .awardSection div {flex: 0 0 40%;width: 100%;margin: 0;}
            .overview-tab .awardSection img {width:100%;margin-top:0;display:block;} 
            .overview-tab .awardSection img#boces2021 {width:50%;margin: 0 auto;}
            .overview-tab .awardSection img#cesia2020 {width:56%;margin: 0 auto;}
            /* Crisis Text Line */
            .overview-tab .crisisTextInfo ul li {color:#666;font-size:1em;}
            .overview-tab .crisisTextInfo h2{ margin-bottom: 8px; }
            .crisisTextInfo h2:nth-of-type(2){ margin-top: 30px; }
            .overview-tab .crisisTextInfo h3 {margin-top:20px;font-size:1.2em;color:#0099cc;}
            .crisisTextInfo .strongPoints {margin-top: 30px;}
            .crisisTextInfo p{ margin-top: 8px; }
            .overview-tab .crisisTextInfo h2 {color:#0099cc;}
            .crisisTextInfo ul{ list-style: disc; margin-left: 30px;} 
            .crisisTextInfo ul li{ font-size:1.25em; line-height:1.4; margin-top:10px; color: #666;}        
            /* Disclaimers */
            .feature-content-tab .disclaimer {font-style: italic;font-size: 80%;margin-top:2em;}


            @media only all and (max-width: 767px) {
                /* Award Section */
                .overview-tab .awardSection div {margin-right:auto;margin-left:auto;width:80%;}
                .overview-tab .awardSection {display:flex;flex-direction:column;}
                .overview-tab .awardSection img#boces2021 {width:100%;}
                .overview-tab .awardSection > div:nth-child(2) {margin-top:50px;}
            }
            /* PHONE (SMALL) */
            @media only all and (max-width: 480px) {
                .feature-content-tab {padding-bottom:5em;}
            }
        }
    }
}

.pr-snippet-stars-reco-reco{
    display: none;
}

.TabSection-section{
    .feature-tab{}
    @media screen and (max-width: 768px) {
        padding-left: 0;
        padding-right: 0;
    }
}

#CartDrawer{
    .drawer__fixed-header{
        height: auto;
        border-bottom: 1px solid var(--colorBorder);
        .drawer__header{
