[data-section-type="mossy-image-text"] {
    max-width: 900px;
    margin: auto;
    border: 6px solid #6A6C68;
    background-size: cover;
    @media screen and (max-width: 768px) {
        background-image: none!important;
        border: none;
        position: relative;
        padding: 0 20px;
        max-height: 500px;
        overflow: hidden;
        padding-bottom: 50px;
        &.active{
            max-height: initial;
            overflow: initial;
            padding-bottom: 20px;
            &::after{
                content: none;
            }
        }
        &::after{
            content: " ";
            width: 100%;
            height: 120px;
            background: linear-gradient(180deg,rgba(255, 255, 255, 0) 0%,rgba(255, 255, 255, 0.86) 19%,white 100%);
            position: absolute;
            bottom: 0;
            left: 0;
        }
        a{
            &.border-btn{
                position: absolute;
                bottom: 0;
                z-index: 10;
                width: calc(100% - 40px);
                background: white;
                border: 1px solid #005F9B;
                color: #005F9B;
                font-weight: 900;
                letter-spacing: normal;
                font-size: 13px;
                position: absolute!important;

            }
        }
    }
    .heading{
        margin-bottom: 50px;
        h2{
            width: fit-content;
            margin: auto;
            transform: translateY(-50%);
            background: white;
            padding: 5px 10px;
            color: #6A6C68;
            @media screen and (max-width: 768px) {
                transform: none;
            }
        }
    }
    .feature-row-wrapper{
        .feature-row{
            flex-direction: row!important;
            @media screen and (max-width: 768px) {
                align-items: flex-start;
                margin-bottom: 20px;
            }
            .feature-row__item{
                flex: 0 1 50%;
                min-width: initial;
                @media screen and (min-width: 768px) {
                    background-image: none!important;
                }
                @media screen and (max-width: 768px) {
                    flex: 0 1 40%;
                }
                &.feature-row__text {
                    flex: 0 1 50%;
                    @media screen and (max-width: 768px) {
                        flex: 0 1 60%!important;
                        padding: 0;
                        padding-left: 10px;
                    }
                    .h1{
                        font-size: 20px;
                        text-transform: unset;
                    }
                    .rte{
                        line-height: 25px;
                        font-size: 18px;
                    }
                }
            }
        }
    }
    .bottom_contnet{
        margin-top: 50px;
        .btn{
            width: 200px;
            background: #007E9E;
        }
        .bottom{
            font-size: 24px;
            text-transform: uppercase;
            font-weight: 900;
            padding: 30px;
            background: #d9d9d9;
            margin-top: 40px;
        }
    }
}

.mobile-hide--true{
    @media screen and (max-width: 768px) {
        display: none;
    }
}

